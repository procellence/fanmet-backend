{
  "name": "functions",
  "version": "0.0.0",
  "scripts": {
    "build": "tsc --project tsconfig.functions.json",
    "build:watch": "tsc --project tsconfig.functions.json --watch",
    "db:update": "./import-db-data.sh",
    "serve": "firebase emulators:start --only functions,firestore,pubsub --import ./db-data",
    "serve:debug": "yarn serve --inspect-functions",
    "proxy:dev": "./cloud-sql-proxy fuelerp-dev:us-central1:postgres-instance",
    "shell": "yarn build && firebase functions:shell",
    "test:ci:old": "jest --runInBand --detectOpenHandles",
    "test:ci": "firebase emulators:exec \"yarn test --ci --color --coverage --silent\" --only firestore",
    "test": "jest --maxWorkers 1",
    "start": "yarn shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "18"
  },
  "main": "lib/src/index.js",
  "dependencies": {
    "@google-cloud/bigquery": "7.3.0",
    "@google-cloud/pubsub": "4.0.6",
    "@google-cloud/secret-manager": "5.0.1",
    "@google-cloud/tasks": "4.0.1",
    "@sendgrid/mail": "7.7.0",
    "adm-zip": "^0.5.10",
    "axios": "1.6.1",
    "basic-ftp": "5.0.3",
    "big.js": "6.1.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "csv-parse": "5.5.2",
    "csv-writer": "1.6.0",
    "deep-object-diff": "1.1.9",
    "firebase-admin": "11.11.1",
    "firebase-functions": "4.6.0",
    "format-string-by-pattern": "1.2.2",
    "fs-extra": "10.0.0",
    "knex": "^2.4.2",
    "lodash": "4.17.21",
    "luxon": "3.4.4",
    "moment": "2.29.4",
    "mustache": "4.2.0",
    "pdf-lib": "1.17.0",
    "pg": "8.11.3",
    "puppeteer": "13.7.0",
    "reflect-metadata": "0.1.13",
    "sharp": "0.32.6",
    "source-map-support": "0.5.21",
    "ssh2-sftp-client": "9.1.0",
    "tsconfig-paths": "4.2.0",
    "typedi": "0.10.0",
    "underscore.string": "3.3.5",
    "uuid": "9.0.1",
    "zipcode-to-timezone": "0.0.9",
    "zod": "3.22.4",
    "zod-form-data": "2.0.2",
    "sprintf-js": "1.1.3",
    "sanitize-filename": "1.6.3",
    "busboy": "1.6.0",
    "mongodb": "6.5.0"
  },
  "devDependencies": {
    "@ag-grid-community/core": "30.2.1",
    "@ag-grid-enterprise/server-side-row-model": "30.2.1",
    "@types/adm-zip": "^0.5.5",
    "@types/big.js": "6.1.2",
    "@types/csv-parse": "1.2.2",
    "@types/fs-extra": "9.0.12",
    "@types/jest": "29.5.8",
    "@types/lodash": "4.14.201",
    "@types/luxon": "3.4.2",
    "@types/moment": "2.13.0",
    "@types/mustache": "4.1.2",
    "@types/pg": "8.10.9",
    "@types/ssh2-sftp-client": "9.0.3",
    "@types/underscore.string": "0.0.38",
    "@types/uuid": "9.0.7",
    "@types/zipcode-to-timezone": "0.0.2",
    "@types/sprintf-js": "1.1.4",
    "@types/busboy": "1.5.0",
    "firebase-functions-test": "3.1.0",
    "jest": "29.7.0",
    "jest-extended": "4.0.2",
    "jest-junit": "16.0.0",
    "ts-jest": "29.1.1",
    "typescript": "5.1.6"
  },
  "private": true
}
